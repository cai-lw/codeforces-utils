{
  "extension": {
    ".cpp": {
      "command": "cpp17",
      "template": "default.cpp"
    },
    ".py": {
      "command": "python3",
      "template": "default.py"
    },
    ".java": {
      "command": "java",
      "template": "default.java"
    }
  },
  "command": {
    "cpp17": {
      "type": "compile",
      "compile": "g++ -O3 --std=c++17 {} -o {}",
      "execute": "{}"
    },
    "python3": {
      "type": "interpret",
      "execute": "python3 {}"
    },
    "java": {
      "type": "jvm",
      "compile": "javac {}",
      "execute": "java {}"
    }
  }
}